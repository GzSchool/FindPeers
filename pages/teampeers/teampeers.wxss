page {
  background-color: #f8f8f8;
}

.search {
  padding: 13rpx 40rpx 16rpx;
}

.search .search-in {
  background-color: #fff;
  border-radius: 6rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  align-items: center;
  padding-left: 83rpx;
}

.search .search-in image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 12rpx;
}

.search .search-in input {
  flex: 1;
  font-size: 26rpx;
  color: #101010;
  height: 100%;
  line-height: 100%;
}

.main {
  background-color: #fff;
}

.main .mine-title {
  height: 70rpx;
  line-height: 70rpx;
  color: #323232;
  font-size: 30rpx;
  text-align: center;
  border-bottom: 1rpx solid #d9d8d9;
}
.mine-card{
  background-color: #ffffff;
  /* border-bottom: 1rpx solid transparent; */
  margin-bottom: 18rpx;
  box-shadow: 0rpx 2rpx 10rpx #eeeeee;
}
.mine-card.res-card{
  margin-bottom: 0;
  padding-bottom: 0;
}
