.crowd-title {
  height: 68rpx;
  line-height: 68rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 0 40rpx;
  justify-content: space-between;
  color: #606060;
}

.crowd-title .crowd-title-left {
  display: flex;
  align-items: center;
}
.crowd-title .crowd-title-left image {
  margin-right: 8rpx;
}

.crowd-title .crowd-title-middle {
  text-align: center;
  flex: 1;
}

.crowd-title .crowd-title-right {
  color: #606060;
  width: 60rpx;
  text-align: center;
}

.crowd-title .crowd-title-right.active {
  color: #2c7fe7;
  background-color: transparent;
  font-size: 28rpx;
  width: 60rpx;
  padding: 0;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
}

/* checkbox */

.select {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  /* background-color: #2C7FE7; */
  margin-right: 4rpx;
}

