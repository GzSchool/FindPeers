/* pages/inputSearch/inputSearch.wxss */
page {
  display: block;
  min-height: 100%;
  background-color: #D6D7DC;
}
.inputSearch{
  border: 1px solid #D6D7DC;
  border-radius: 3px;
  position: absolute;
  top: 10px;
  left: 5%;
  width:90%;
  height:40px;
  display:inline-block;
  background:#fff;
}
.image{
  display:inline;
}
.search{
  width:80%;display:inline-block;height:100%;
  position: absolute;
  left: 23%;
  top: 0px;
}
.search1{
  width:20%;display:inline-block;height:40%;
  position: absolute;
  left: 5%;
  top: 30%;
}
.item{
  position: relative;
  top: 67px;
  left: 0; 
  width: 100%;
  height: 20%;
  background-color: #ffffff;
}
.find{
  background-color: #ffffff;
  width: 100%;
  height: 70%;
  position: relative;
  top: 10px;
  left: 100;
}
.url{
  display: flex;
  flex-direction: row;
  width: 80px;
  height: 80px;
  margin-top: 15px;
  margin-left: 20px;
  border-radius: 10px;
}
.name{
 display: flex;
 flex-direction: row;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  font-size: 20px;
  text-align: left;
  color: #505050;
}
.industry{
  display: flex;
  flex-direction: row;
  font-size: 12px;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  text-align: left;
  color: #505050;
}
.city{
   display: flex;
   flex-direction: row;
  font-size: 16px;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  text-align: left;
  color: #505050;
}
.company{
   display: flex;
   flex-direction: row;
  font-size: 16px;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  text-align: left;
  color: #505050;
}