/* pages/quncards/quncards.wxss */

page {
  display: block;
  min-height: 100%;
  background-color: #d6dbe0;
}
.qun{
  width: 100%;
  height: 100%;
}
.qunbiao{
  width: 100%;
  height: 20px;
  position: relative;
  top: 10px;
  left: 0;
  background-color: white;
}
.shu{
  color: #2c7fe7;
  width: 1px;
  height: 100%;
  position: relative;
  left: 20px;
}
.text{
  font-size: 15px;
  position: relative;
  left: 50px;
  }
.list{
    position: relative;
    top: 1px;
    left: 0;
    width: 100%;
    height: 100%;
}
.item{
    margin-top: 5px;
  display: flex;
  flex-wrap: wrap;
  position: relative;
  width: 100%;
  height: 20%;
  background-color: #ffffff;
}
.find{
  display: flex;
  background-color: #ffffff;
  width: 100%;
  height: 70%;
  position: relative;
  top: 10px;
  left: 100;
}
.url{
  display: flex;
  flex-direction: row;
  width: 80px;
  height: 80px;
  margin-top: 15px;
  margin-left: 20px;
  border-radius: 10px;
}
.name{
 display: flex;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  font-size: 20px;
  text-align: left;
  color: #505050;
  margin-top: 20px;
  margin-left: 20px;
}
.industry{
  display: flex;
  font-size: 12px;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  text-align: left;
  color: #505050;
}
.city{
   display: flex;
  font-size: 16px;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  text-align: left;
  color: #505050;
}
.company{
  font-size: 16px;
  display: block;
  font-style: MicrosoftYaHei;
  font-weight: Regular;
  color: #505050;
  margin-top: 60px;
}