<!--pages/quncards/quncards.wxml-->
<view class="qun">
  <view class="qunbiao">
  <button class="shu"></button>
  <view class="text">群列表</view>
  </view>
  <scroll-view scroll-top="{{scrollTop}}" scroll-y="true" style="height:{{scrollHeight}}px;" class="list" bindscrolltolower="bindDownLoad" bindscroll="scroll" bindscrolltoupper="refresh">
    <view class="item" wx:for="{{list}}" wx:for-index="index" wx:for-item="item" wx:key="11" data-key="{{item.groupId}}" data-id="{{item.openId}}" bindtap='search'>
      <!--<image class="img" src="{{item.pic_url}}"></image>-->
      <view class="find">
      <image class="url" src="/pages/images/1.png"></image>
        <view class="name">{{item.groupId}}</view>
        <view class="company">{{item.delFlag}}</view>
      </view>
    </view>
  </scroll-view>
  
</view>
