/* pages/industry/industry.wxss */
page{
  background-color: #f8f8f8;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.main {
  background-color: #ffffff;
  display: flex;
  font-size: 30rpx;
  color: #101010;
  height: 100%;
  overflow: hidden;
}
.main .main-nav {
  /* min-width: 200rpx; */
  padding: 0 30rpx;
  text-align: center;
  background-color: #fafafa;
  overflow: auto;
}
.main .main-nav .main-nav-li {
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid #ffffff;
}
.main .main-content {
  flex: 1;
  overflow: auto;
}
.main .main-content .main-content-li {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-bottom: 1px solid #fffffa;
}