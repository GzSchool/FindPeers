/**app.wxss**/
@import 'style/weui.wxss';
@import 'style/card.wxss';
page{
  /* background-color:"#d6dbe0"; */
  width: 100%;
  /* height:100%; */
  font-family: 'Microsoft YaHei';
}
view {
  box-sizing: border-box;
}
button::after{
  border:none;
}
.block{
    display: block;
}
.flex{
    display: flex;
}

.flex-1{
    flex:1;
}

.align-center{
    align-items: center;
}
.align-content{
    align-content:space-between;
}

.flex-wrap{
    flex-wrap: wrap;
}
.flex-flow-column{
    flex-flow:column;
}

.justify-content-between{
    justify-content: space-between;
}

.justify-center{
    justify-content: center;
}

.nowrap{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.mt20{
    margin-top: 20rpx;
}
.mb20{
    margin-bottom: 20rpx;
}
.mr20{
    margin-right: 20rpx;
}
.ml20{
    margin-left: 20rpx;
}
.w100{
    width: 100%;
    box-sizing:border-box;
}
.h100{
    height: 100%;
    box-sizing:border-box;
}